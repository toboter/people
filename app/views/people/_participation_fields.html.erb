<div class="nested-fields row">
  <div class="col-md-10">
    <%= f.input :from, as: :date, include_blank: true, start_year: 1897, end_year: 1917, order: [:day, :month, :year] %>
    <%= f.input :till, as: :date, include_blank: true, start_year: 1897, end_year: 1917, order: [:day, :month, :year] %>
